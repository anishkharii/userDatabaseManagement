<%- include("partials/header") -%>


<div class="main-head">
    <a class="primary-btn new-user-btn" href="/dashboard/<%=admin%>/newUser">Add User</a>
    <div class="admin-detail">
        <h5><%=admin%></h5>
        <a class="primary-btn warning-btn" href="/">Log Out</a>
    </div>
</div>

<table class="user-details">
     <thead>
        <tr>
            <td>ID</td>
            <td>Name</td>
            <td>Email</td>
            <td>Gender</td>
            <td>Status</td>
            <td>Added By</td>
            <td>Edit</td>
        </tr>
     </thead>
     <tbody>
        <% users.forEach((user)=>{%>
            <tr>
                <td><%=user._id%></td>
                <td><%=user.name%></td>
                <td><%=user.email%></td>
                <td><%=user.gender%></td>
                <td><%=user.status%></td>
                <td><%=user.updatedBy.userID%></td>
                <td>Edit</td>
            </tr>
        <% })%>
        
    </tbody>
</table>

<%- include("partials/footer") -%>